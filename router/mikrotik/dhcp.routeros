/ip pool
add name=default-dhcp ranges=192.168.1.1-192.168.254.254
/ip dhcp-server
add address-pool=default-dhcp authoritative=after-2sec-delay bootp-support=\
    static disabled=no interface=ether2-master-local lease-time=3d name=\
    default
/ip dhcp-client
add add-default-route=yes comment="internal router" \
    default-route-distance=1 disabled=no interface=ether1-gateway \
    use-peer-dns=yes use-peer-ntp=yes
/ip dhcp-server config
set store-leases-disk=5m
/ip dhcp-server network
add address=0.0.0.0/0 dhcp-option="" dns-server="" gateway="" ntp-server="" \
    wins-server=""
add address=192.168.0.0/16 comment="default configuration" dhcp-option="" \
    dns-server=192.168.0.10,208.201.224.11,208.201.224.33 domain=cloyne \
    gateway=192.168.0.1 ntp-server=192.168.0.10 wins-server=""
